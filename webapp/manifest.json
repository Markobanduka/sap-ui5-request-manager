{
  "_version": "1.76.0",
  "sap.app": {
    "id": "ui5.requestmanager",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "0ef15bf0-bc13-42c3-9354-91a8939c3feb"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "componentName": "ui5.requestmanager",
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ui5.requestmanager.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ui5.requestmanager.view",
        "async": true,
        "viewPath": "ui5.requestmanager.view"
      },
      "routes": [
        {
          "pattern": "",
          "name": "dashboard",
          "target": "dashboard"
        },
        {
          "pattern": "create",
          "name": "create",
          "target": "create"
        },
        {
          "pattern": "myrequests",
          "name": "myrequests",
          "target": "myrequests"
        },
        {
          "pattern": "details/{requestId}",
          "name": "details",
          "target": "details"
        },
        {
          "pattern": "stats",
          "name": "stats",
          "target": "stats"
        }
      ],
      "targets": {
        "dashboard": {
          "viewName": "Dashboard",
          "viewId": "Dashboard"
        },
        "create": {
          "viewName": "CreateRequest",
          "viewId": "CreateRequest"
        },
        "myrequests": {
          "viewName": "MyRequests",
          "viewId": "MyRequests"
        },
        "details": {
          "viewName": "RequestDetails",
          "viewId": "RequestDetails",
          "transition": "slide"
        },
          "stats": {
          "viewName": "Statistics",
          "viewId": "Statistics",
          "transition": "fade"
        }
      }
    },
    "rootView": {
      "viewName": "ui5.requestmanager.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
